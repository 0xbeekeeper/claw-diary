{
  "name": "claw-diary",
  "version": "1.1.0",
  "description": "Personal AI agent visual diary. Auto-records all agent activity, generates daily narrative summaries, and provides visual timeline replay.",
  "author": "0xbeekeeper",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xbeekeeper/claw-diary.git"
  },
  "homepage": "https://github.com/0xbeekeeper/claw-diary",
  "bugs": {
    "url": "https://github.com/0xbeekeeper/claw-diary/issues"
  },
  "keywords": [
    "ai-agent",
    "diary",
    "timeline",
    "cost-tracking",
    "analytics",
    "claude",
    "openai",
    "llm",
    "clawhub",
    "openclaw"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "claw-diary": "dist/scripts/cli.js"
  },
  "files": [
    "dist/",
    "SKILL.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "collect": "node dist/scripts/collector.js",
    "summarize": "node dist/scripts/summarizer.js",
    "timeline": "node dist/scripts/timeline.js",
    "analytics": "node dist/scripts/analytics.js",
    "server": "node dist/scripts/server.js",
    "test": "node --test dist/tests/*.js"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.3.0"
  }
}
